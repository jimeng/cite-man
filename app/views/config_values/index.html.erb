<h1>Listing config_values</h1>

<table>
  <tr>
    <th>Source type</th>
    <th>Name</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @config_values.each do |config_value| %>
  <tr>
    <td><%= config_value.source_type %></td>
    <td><%= config_value.name %></td>
    <td><%= config_value.value %></td>
    <td><%= link_to 'Show', config_value %></td>
    <td><%= link_to 'Edit', edit_config_value_path(config_value) %></td>
    <td><%= link_to 'Destroy', config_value, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Config value', new_config_value_path %>
